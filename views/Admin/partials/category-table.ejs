<% categories.forEach((data) => {  %>
    <tr>
        <td class=" opacity-70 text-sm">1</td>
        <td class=" opacity-70 capitalize text-sm"><%= data.categoryname %></td>
        <td class=" opacity-70 w-1/3 text-sm"><%= data.productscount %></td>
        <td><div
            <% if(data.showstatus === "Listed") { %>
             class="bg-green-100 text-green-700 rounded-full my-1 text-sm" 
             <% }else{ %>
                class="bg-red-100 text-red-700 rounded-full my-1 text-sm"
                <% } %>
             ><%= data.showstatus
              %></div></td>
        <td class=" opacity-70">
            <button onclick="editpopup(this)" id=" <%= data._id %> "><img src="/images/edit.png" class="h-6" alt=""></button>
            <button id="<%= data._id %>-<%= data.categoryname %>" onclick="deletepopup(this)"><img src="/images/delete.png" class="h-6" alt=""></button>
        </td>
    </tr>
    <% }) %> 