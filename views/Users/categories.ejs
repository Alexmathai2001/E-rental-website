                            <p class="py-1 text-sm max-w-[100px] truncate"><%= products[i].productname %></p>
                        </div>
                        <div>
                            <p class="text-xs opacity-50 max-w-[50px] truncate"><%= products[i].brandname %></p>
                        </div>
                        <div class="flex justify-between w-full items-center lg:my-6">
                            <div>
                                <span class="font-semibold text-sm">$ <%= products[i].regularprice %>/day</span>
                            </div>
                            <div>
                                <button class="p-1 bg-[#27374D] rounded-full"><img src="/images/cart-white.png" class="h-6 p-[2px]" alt=""></button>
                            </div>
                        </div>
                    </div> 
