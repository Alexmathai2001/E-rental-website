    <main class="my-2 mx-3 lg:grid lg:grid-cols-2 lg:gap-5">
        
        <!--category 1-->
        <div class="my-4 w-full ">
            
            <p class="text-sm font-medium opacity-75 lg:text-lg lg:font-semibold md:text-base my-4">Cutting Tools</p>
            <!--cards-->
            <div class="flex">
